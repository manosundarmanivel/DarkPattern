{
  "manifest_version": 3,
  "name": "Dark Pattern Finder",
  "version": "1.0",
  "description": "Extracts the Dark Pattern of the current tab",
  "permissions": [
    "activeTab"
  ],
  "action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "icons": {
    "48": "logo.png"
  }
}